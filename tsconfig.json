{
  "compilerOptions": {
    "rootDir": "#src",
    "module": "commonjs",
    "target": "es6",
    "sourceMap": true,
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "allowJs": true
  },
  "include": [
    "./#src"
  ],
  "exclude": [
    "node_modules",
    "build"
  ]
}